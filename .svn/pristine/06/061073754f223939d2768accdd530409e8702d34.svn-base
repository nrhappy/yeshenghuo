a {
  color:#6a6a6a;
}
a:hover {
  color:#6a6a6a;
  text-decoration:none;
}

.left-side-bar {
  min-height:100%;
}

.left-side-bar ul {
  padding:17px 30px;
  border-bottom:1px solid #ddd;
  list-style-type:none;
}

.left-side-bar ul:last-child {
  border-bottom:0px;
}

.left-side-bar ul li {
  padding-top:10px;
  padding-left:22px;
}

.item-head {
  font-size:16px;
}

.licon {
  width:22px; height:22px; display:inline-block; position:relative; top:3px; right:10px;"
}